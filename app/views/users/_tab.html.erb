<ul class="nav nav-tabs" id="myTab" role="tablist">
  <li class="nav-item">
    <a class="nav-link active" id="home-tab" data-toggle="tab" href="#home" role="tab" aria-controls="home" aria-selected="true">Closet</a>
  </li>
  <li class="nav-item">
    <a class="nav-link" id="profile-tab" data-toggle="tab" href="#profile" role="tab" aria-controls="profile" aria-selected="false">Looks</a>
  </li>
</ul>
<div class="tab-content" id="myTabContent">
  <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
    <% @user.items.each do |item| %>
      <% if item.photo.attached? %>
        <%= link_to item_path(item) do %>
          <%= cl_image_tag item.photo.key, height: 250, width: 300, crop: :fill %>
        <% end %>
      <% else %>
        <%= image_tag "blouse.jpg", style: "height: 300px; width: 250px;"%>
      <% end %>
    <% end %>

  </div>
  <div class="tab-pane fade" id="profile" role="tabpanel" aria-labelledby="profile-tab">
    <!-- put looks  -->
    <h1> looks</h1>
  </div>
</div>
